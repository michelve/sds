{
  "name": "@sds/ui-angular",
  "version": "1.0.0",
  "description": "Angular component library for the SDS Design System",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "@sds/design-tokens": "*",
    "@angular/core": ">=17.0.0",
    "@angular/common": ">=17.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michelve/sds.git",
    "directory": "packages/ui-angular"
  },
  "keywords": [
    "angular",
    "components",
    "design-system",
    "sds"
  ],
  "license": "MIT",
  "module": "fesm2022/sds-ui-angular.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/sds-ui-angular.mjs",
      "esm": "./esm2022/sds-ui-angular.mjs",
      "default": "./fesm2022/sds-ui-angular.mjs"
    }
  },
  "sideEffects": false
}